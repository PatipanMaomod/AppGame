+import javax.swing.*;
+import java.awt.event.ActionEvent;
+import java.awt.event.ActionListener;
+
+public class PlayerNameWindow extends JFrame {
+
+    private JTextField nameField;
+    private JButton submitButton;
+    private JLabel nameLabel;
+    private JPanel wi;
+    private JTextField textField1;
+
+    public PlayerNameWindow() {
+        setTitle("Enter Player Name");
+        setDefaultCloseOperation(JFrame.DISPOSE_ON_CLOSE);
+        setSize(800, 600);
+        setLocationRelativeTo(null);
+        setContentPane(wi);
+
+        setVisible(true);
+        submitButton.addActionListener(new ActionListener() {
+            @Override
+            public void actionPerformed(ActionEvent e) {
+                handleSubmit();
+                openNextWindow();
+            }
+        });
+    }
+    private void openNextWindow() {
+        // สร้างหน้าต่างถัดไปและทำให้มันมองเห็นได้
+        MainGUI mg = new MainGUI();
+        //เปิดหน้า Arana
+        mg.setVisible(true);
+        //ปิดหน่้านี้
+        setVisible(false);
+    }
+
+    private void handleSubmit() {
+        String playerName = nameField.getText();
+        if (!playerName.trim().isEmpty()) {
+            AranaFrame aranaFrame = new AranaFrame();
+            aranaFrame.setVisible(true);
+            setVisible(false);
+        } else {
+            JOptionPane.showMessageDialog(this, "Name cannot be empty!");
+        }
+    }
+
+    public static void main(String[] args) {
+        SwingUtilities.invokeLater(() -> new PlayerNameWindow());
+    }
+}
